<p id="notice"><%= notice %></p>
<div id="container">
  <div class="table">
    <h1>Vendors</h1><table>
      <thead>
        <tr>
          <th>Name</th>
        </tr>
      </thead>
      <tbody>
      <% @vendors.each do |vendor| %>
          <tr>
            <td><%= vendor.vendor_name %>
            <td><%= link_to 'Edit', edit_vendor_path(vendor) %></td>
            <td><%= link_to 'Destroy', vendor_path(vendor), method: :delete, data: { confirm: 'Are you sure?' }, class: 'crud' %></td>
          <% end %>
          </tr>
      </tbody>
    </table>
    <%= link_to 'New', new_vendor_path, class: 'crud' %>
    <br>
</div></div>